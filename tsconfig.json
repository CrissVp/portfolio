{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@styles/*": [
        "src/styles/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@i18n/*": [
        "src/i18n/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  }
}